<h4>Worker Details</h4>
<table  class="hibernate-worker">
    <tr>
        <th>Key</th>
        <th>Value</th>
    </tr>
    <tr>
        <td>ID</td>
        <td>{{worker.id}}</td>
    </tr>
    <tr>
        <td>Fname</td>
        <td>{{worker.fname}}</td>
    </tr>
    <tr>
        <td>Lname</td>
        <td>{{worker.lname}}</td>
    </tr>
    <tr>
        <td>Country</td>
        <td>{{worker.country}}</td>
    </tr>
    <tr>
        <td>City</td>
        <td>{{worker.city}}</td>
    </tr>
    <tr>
        <td>PLZ</td>
        <td>{{worker.plz}}</td>
    </tr>
    <tr>
        <td>Age</td>
        <td>{{worker.age}}</td>
    </tr>
    <tr>
        <td>Wage</td>
        <td>{{worker.wage}}</td>
    </tr>
    <tr>
        <td>Active</td>
        <td>{{worker.active}}</td>
    </tr>
</table>